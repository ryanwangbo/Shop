<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="entity">
	<class name="entity.User" table="t_user">
		<id name="id">
			<column name="t_id"></column>
			<generator class="sequence"></generator>
		</id>
		<property name="username" column="t_username" type="java.lang.String"></property>
		<property name="password" column="t_password" type="java.lang.String"></property>
		<property name="email" column="t_email" type="java.lang.String"></property>
	</class>
	
	
</hibernate-mapping>